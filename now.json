{
  "name": "github-code-review-notifier",
  "version": 2,
  "builds": [
    { "src": "src/routes/test.js", "use": "@now/node", "config": { "maxLambdaSize": "6mb" } },
    { "src": "src/routes/bot.js", "use": "@now/node", "config": { "maxLambdaSize": "6mb" } }
  ],
  "routes": [
    {
      "src": "/bot",
      "dest": "src/routes/bot.js",
      "headers": {
        "Content-type": "application/json"
      }
    },
    {
      "src": "/test",
      "dest": "src/routes/test.js",
      "headers": {
        "Content-type": "application/json"
      }
    }
  ],
  "env": {
    "FIREBASE": "@firebase",
    "VERIFICATION_TOKEN": "@verification_token",
    "CLIENT_SECRET": "@client_secret",
    "CLIENT_ID": "@client_id",
    "SIGNING_SECRET": "@signing_secret",
    "NODE_ENV": "production"
  },
  "alias": "https://github-code-review-notifier.now.sh"
}
