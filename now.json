{
  "name": "github-code-review-notifier",
  "env": {
    "FIREBASE": "@firebase",
    "VERIFICATION_TOKEN": "@verification_token",
    "CLIENT_SECRET": "@client_secret",
    "CLIENT_ID": "@client_id",
    "SIGNING_SECRET": "@signing_secret",
    "NODE_ENV": "production"
  },
  "routes": [
    { "src": "/bot", "dest": "/api/bot.ts" },
    { "src": "/github", "dest": "/api/github.ts" },
    { "src": "/oauth", "dest": "/api/oauth.ts" },
    { "src": "/interactive-components", "dest": "/api/interactive-component.tss" }
  ],
  "alias": "https://github-code-review-notifier.now.sh"
}
